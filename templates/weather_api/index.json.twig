{
    "city": "Szczecin",
    "country": "PL",
    "measurements": [
        {% for measurement in measurements %}
        {
            "date": "{{ measurement.DateOfMeasurement | date('Y-m-d') }}",
            "celsius": "{{ measurement.temperature }}",
            "fahrenheit": "{{ measurement.getFahrenheit()}}"
        }{% if not loop.last %},{% endif %}
        {% endfor %}
    ]
}